@charset "utf-8";
*{
    padding: 0;
    margin:0 auto;
    box-sizing: border-box;
    -webkit-box-sizing: border-box; 
    color: gray;   
}
body{
    background-color: #fff;
}
header{
    margin-top: 20px;
}
.aui-card-list{
    margin-bottom: 0px !important;
}
.aui-card-list{
    padding: 0 !important;
}
.aui-card-list-header{
    padding:0 !important; 
}
.aui-card-list-header img{
    height:170px;
    width:100%;
}
.aui-card-list-header i{
    margin-right:20px; 
}
section{
    padding:10px 20px 0; 
}
div.foot{
    width:100%;
    overflow: hidden;
    -webkit-overflow-scrolling: touch;
    overflow-y: hidden;
    overflow-x:scroll;
}
div.foot>div{
    width:800px;
    height:100%; 
}
div.foot>div>div>img{
    height:100px;
    width:100px;
    float: left;   
}
div.deser{
    float: left;
    margin:10px 10px 0 0;  
    z-index: 10;
    position: relative;
}
div.zhen{
    position: absolute;
    top:0px;left:0;
    background-color:rgba(0, 0, 0,0.1) ;
    width:100%;
    height:100%;  
}
div.zhen span{
    float: left;
    background-color: red;
    color: #fff;
    font-size:12px; 
    padding:2px 5px;
}
div.zhen div{
    width:100px;
    color: #fff;
    margin-top:35px;
    text-align:center;
    font-size:0.75rem; 
}
.aui-card-list-user-name div{
    color:#000; 
    font-size:0.8rem;
}
.aui{
    color: #000;
}
.aui i{
    margin-right:20px;
    float: right; 
}
small{
    margin-right:10px;
}
small i{
    margin-left:20px; 
}
.aui-card-list-user-avatar {
    width: 2rem !important;
}
.code{
    margin:0 10px; 
}
.code div{
    width:100%;
}
.code div:first-child{
    font-size:0.8rem;
    color: #000;
}
.code div:nth-child(2){
    font-size:0.65rem; 
}
.zero{
    color: #000;
    margin-right:10px; 
    font-size:14px; 
}
.photo,ul{
    background-color: #ececec !important;
}
ul li img{
    margin-left:5px;
}
.aui-row-padded {
    margin-left: 10px !important;
    margin-right: -0.125rem;
}
.photo img{
    width:60px !important;
}
.aui-list .aui-list-item-media {
    width: 4rem !important;
}
.aui-iconfont {
    font-size: 1rem;
}
#picOne{
    width:100%;
    padding:10px 0 0 10px; 
    overflow: hidden;
}
#picOne:after{
    clear: both;
    content: "";
    width: 0;
    height: 0;
    display: block;
    visibility: hidden;
}
#picOne img{
    width:33.3%;
    display: inline-block;
    float: left;
    padding:0px 10px 10px 0px;
}
#picOne img:nth-child(3){
    margin-top:-24px; 
}
.mask-img {
    display: none; 
    position: fixed; 
    top: 0; 
    left: 0; 
    width: 100%; 
    height: 100%; 
    z-index: 10; 
    background: rgba(0,0,0,.5);
} 
.picture{
    display: none; 
    position: fixed; 
    top:50%; left:60%; 
    transform: translate(-50%,-50%); 
    width:100%;  
    line-height:100%; 
    text-align:center; 

    z-index: 20;
  }
  .picture .phone{
    vertical-align: middle; 
    max-width: 80%; 
    max-height:80%; 
  }
  .picture .left{
    position: absolute; 
    left: 10px; 
    top: 320px; 
    width: 25px; 
    height: 40px; 
    line-height: 40px;
  }
  .picture .right{
    position: absolute; 
    right: 10px; 
    top: 320px; 
    width: 25px; 
    height: 40px; 
    line-height: 40px;
  }
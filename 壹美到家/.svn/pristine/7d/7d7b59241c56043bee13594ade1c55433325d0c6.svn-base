*{
    padding: 0;
    margin:0 auto;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
    font-size: 12px;
}

.classhead{
  margin-top:20px;
  position: fixed;
  top:0px;
  left: 0px;
  padding-left: 5px;
  background: transparent;
}
.classhead.aui-bar-nav.aui-bar-light .aui-iconfont {
    color: #34352f;
}
.classhead.aui-bar.aui-bar-light {
    background-size: 100% 0px;
}
 .headbg img{
  width: 100%;
}
main{
    background: #fff;
}
main h3{
  border-bottom: 1px solid #ccc;
  padding: 20px 15px;
  font-size: 14px;
  margin:0 15px ;
}
main p{
  margin:0 20px ;
  line-height: 20px;
  padding: 0 15px;
}
main p{
  margin-top: 10px;
  font-size: 12px !important;
}

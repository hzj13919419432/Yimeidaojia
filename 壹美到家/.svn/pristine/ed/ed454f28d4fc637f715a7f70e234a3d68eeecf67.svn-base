@charset "utf-8";

html,body{
	width: 100%;
	height: 100%;
	background-color: #fff;
	padding:0;
	margin: 0;
}
body{
	background-color: #fff;
}
header{
	padding-bottom: 0;
	position: fixed !important;
}
header.aui-bar{
	background-color: #000;
}
.aui-bar-nav .aui-pull-left {
    padding: 0 1rem;
}
.aui-bar-nav .aui-pull-right {
    padding: 0px 1rem;
}
span.aui-iconfont.aui-icon-left{
	color: #fff;
}
.aui-bar-nav .aui-title {
    right:3rem;
    left:3rem;
}
header > *{
	margin-top:20px !important;
}
.aui-bar-nav .aui-iconfont {
    color: #fff;
}
.aui-tab {
    background-color:rgba(255, 255, 255,0) !important;
}
.aui-tab-item {
    color: #fff;
}
section{
	padding:15px 15px 0 15px;
}
.aui-card-list-header, .aui-card-list-footer {
    padding: 0.5rem 0rem 1rem 0;
}
.aui-card-list-user-avatar {
    width:40px;
}
.aui-card-list-user-name{
	font-size: 12px;
	height:40px;
	line-height:40px;
}
.aui-card-list-user-name small{
	border: 1px solid #e4b118;
	width:45px !important;
	height:25px !important;
	line-height:25px;
	text-align: center;
	border-radius: 5px;
	color:#e4b118;
	font-size:14px;
}
.imags{
	width:60px;
	margin-left:-70px;
}
.imags img{
	width:10px !important;
	float: left;
}
.aui-col-xs-4{
	height:100px;
	position: relative;
}
.aui-col-xs-4.smal {
    width: 25%;
}
.aui-col-xs-4 span{
	background-color:#ececec !important;
	position: absolute;
	 bottom:0px;right:0px;
	z-index: 1;
	color: #000;
	padding:0 5px;
	text-align: center;
	font-size: 14px;
}
.aui-list-item-inner{
	font-size:14px;
}
.aui-card-list-user-name div{
    color:#000;
    font-size:0.75rem;
}
small{
    margin-right:10px;
}
small i{
    margin-left:20px;
    color:red;
}
.aui-card-list-user-avatar {
    width:40px !important;
}
.code{
    margin:0 10px;
}
.code div{
    width:100%;
}
.code div{
	font-size:14px;
	margin-top:5px;
}
.aui-padded-10{
    padding-bottom:0 !important;
}
.zero{
    color: #000;
	margin-right:10px;
	font-size:14px;
}
.photo{
    background-color: #ececec !important;
}
ul li img{
    margin-left:5px;
}
small.tite{
	border: none !important;
}
body{
	position: relative;
}
section{
	position: absolute !important;
	top:0;
	left:0;
	background-color: #fff;
	margin-top:65px;
	height:500px;
	font-size: 14px;
	margin-top:68px;
}
.action{
	z-index: 50;
}
.photo .aui-list .aui-list-item {
	padding-left: 0rem;
	background-color: #ececec !important;
}
.aui-list .aui-list-item-title {
    font-size: 12px;
}
.aui-list .aui-list-item i.aui-iconfont {
    font-size: 20px;
}
.aui-tab-item.aui-active {
    color: #e4b118;
    border-bottom: 2px solid #e4b118;
}
.money{
	margin-top:5px;
}
.money div:first-child{
	color:red !important;
}
ul.two{
	background-color:#fff !important;
}
ul.two li{
	margin-bottom:10px !important;
}
.aui-list .aui-list-item-inner {
    padding-right: 0rem;
}
.aui-list .aui-list-item-right{
    margin-left: 0.5rem;
}
.aui-iconfont {
    font-size: 1rem;
}
.aui-card-list-user-info {
    color:#757575;
	font-size:10px;
	margin-top:5px;
}
.aui-list {
    background-size: 100% 0px !important;
}
.photo{
	margin:0 -15px !important;
}
.photo ul{
	margin-left: 15px;
}
.aui-list .aui-list-item-media {
    width: 4rem;
}

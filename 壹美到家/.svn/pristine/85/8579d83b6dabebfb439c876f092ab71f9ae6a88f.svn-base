@charset "utf-8";
*{
    padding: 0;
    margin:0 auto;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
	color: gray;
}
body{
	background-color: #fff;
}
header{
    background-color: #000 !important;
}
header > *{
	margin-top:20px !important;
}
header .aui-iconfont{
    font-size:16px !important; 
    color: #fff !important;
}
header .aui-title{
    color:#fff;
}
header{
	position: fixed !important;
    top:0;
} 
.aui-content{
    margin-top:65px !important; 
}
.aui-card-list-header {
    padding: 0 !important;
}
.aui-card-list-footer{
    padding:0;
}
.aui-card-list-footer .aui-row{
    width:100%;
}
.home{
	position: relative;
	padding: 1rem 0 0 !important;
}
.home .homes{
	width:30px;
	height:30px;
	position: absolute;
	top:20px;
	left:85px;
	border-right:1px solid gray;
	transform:rotate(15deg);
	-ms-transform:rotate(15deg); 	/* IE 9 */
	-moz-transform:rotate(15deg); 	/* Firefox */
	-webkit-transform:rotate(15deg); /* Safari å’Œ Chrome */
	-o-transform:rotate(15deg);
}
.home div{
	font-size:14px;
	color:#000;
}
.home p{
	color:gray;
	font-size:12px;
}
.aui-card-list-content-padded div:nth-child(1){
    font-size:16px;
    text-align: center;
    color:#000;
}
.aui-card-list-content-padded div:nth-child(2){
    font-size:14px;
    text-align: center;
}
.aui-card-list-footer > div{
    width: 100%;
    overflow: hidden;
    -webkit-overflow-scrolling: touch;
    overflow-y: hidden;
    overflow-x: scroll;
}
.aui-card-list-content-padded {
    padding: .5rem 0.75rem 0.5rem;
}
.amen{
    width: 900px !important;
    height: 100%;
}
.amen > div{
    width: 150px;
    padding: 10px 10px 0 !important;
    float: left;
}
.aui-col-xs-4 > *{
    text-align: center;
    margin-top:5px; 
}
.aui-col-xs-4 p{
    word-break: break-all;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp:1;
    overflow: hidden;
    font-size:12px;    
}
.aui-col-xs-4 div span{
    color:#000;
    font-size:10px; 
}
.aui-col-xs-4 div a{
    font-size: 14px;
    font-weight:700; 
    color:#000;
}
.aui-col-xs-4 p:nth-child(4){
    font-size:12px;
    text-decoration: line-through; 
}
section > div >div:nth-child(2){
    padding:0 0.75rem;
}
.one div{
    font-size:14px;
    background-color: blue;
    text-align: center; 
}
.one div p{
    color:#fff;
    padding-top:5px; 
}
.one div div{
    color:#fff;
    padding:5px 0 5px 0; 
}
.one div div span{
    display: inline-block;
    width:20px;
    height:20px;
    border-radius:50%;
    background-color:#fff; 
    color:blue; 
}
.two{
    padding:10px 0px 10px 0;
}
.two > div{
    border:10px solid red;
    border-left:none;  
}
.two div:nth-child(2){
    border-top:none;
}
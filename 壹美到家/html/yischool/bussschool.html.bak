                <img src="../../image/bussschool/bussschool/f4eec3a904d8d3c8.jpg" /> -->
                <img src="{{topic.topicimage.TI_Url}}" />
            </div>
        </div>
        <!--  遮罩层 -->
        <div class="mask-img"></div>
        <div class="picture">
            <img class="phone" src="" alt="" />
        </div>

*{
    padding: 0;
    margin:0 auto;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
    font-size: 12px;
}

.classhead{
  margin-top:20px;
  position: absolute;
  padding:10px;
  top:0px;
  left: 0px;
  padding-left: 5px;
  background: transparent;
}
.classhead.aui-bar-nav.aui-bar-light .aui-iconfont {
    color: #34352f;
}
.classhead.aui-bar.aui-bar-light {
    background-size: 100% 0px;
}
 .headbg img{
  margin-top: 20px;
  width: 100%;
}
main{
    background: #fff;
}
main h3{
  padding-top:10px;
  border-bottom: 1px solid #ccc;
  padding-bottom: 10px;
  font-size: 14px;
}
main h3,main p{
  padding-left: 20px;
  padding-right: 20px;
  line-height: 30px;
}
main p{
  margin-top: 10px;
  font-size: 12px !important;
}

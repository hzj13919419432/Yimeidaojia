*{
    padding: 0;
    margin:0 auto;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
    color: gray;
}
body{
    background-color: #ddd;
}
header{
    padding-top:20px;
    border-bottom: 1px solid #dddddd;
    position: fixed !important;
    top:0;
}
.aui-bar-nav{
    background-color: #000 !important;
}
header a{
    color: gray !important;
}
header a span{
    color:#fff !important;
}
header div{
    background-color:#000;
    color: #fff;
}
.aui-content{
    background-color:#f5f5f5;
    margin-top:65px; 
}
div.login{
    width:100%;
    height:160px;
    position: relative;
}
div.login>div.img{
    width:100px;
    height:100px;
    position: absolute;
    top:60px;left:50%;
    margin-left: -50px;
}
div.login>div.img>img{
    width:100%;
}
div.text{
    width:100%;
    font-size:14px;
    text-align: center;
    margin-bottom:20px;
    margin-top:10px;
    color:#000; 
}
p{
    color: #000;
    font-size:14px;
    margin-top:10px;
}
.aui-media-list-item-inner{
    padding:15px 1rem;
    border-bottom:1px solid #ececec;
    background-color: #fff;
}
.aui-list-item-media{
    font-size:14px;
    margin-right:50%; 
}
.aui-list-item-inner{
    font-size:14px;
    color:#000;
    margin-top:0.1rem;
}
.aui-list-item-inner a{
    color:orange;
}
.texter{
    margin-top:150px;
    font-size:12px;
    color:#bdbdbd;
    text-align: center;  
    margin-bottom:50px; 
}

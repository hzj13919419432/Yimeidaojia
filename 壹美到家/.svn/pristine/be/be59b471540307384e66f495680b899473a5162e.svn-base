apiready = function () {
  init();
}
function init() {
  api.parseTapmode();
  fixHeaderbar();
  inittab();
}
function fixHeaderbar() {
  var header = document.querySelector('body>header');
  $api.fixStatusBar(header);
}
function toYishow() {
  api.openWin({
      name: 'topic',
      url: '../../html/yishow/topic.html',
      pageParam: {
          name: 'topic'
      }
  });
}
function inittab() {
  var tab = new auiTab({
        element:document.getElementById("tab"),
        index:1,
        repeatClick:false
    },function(ret){
      console.log(ret.index);
    });
}

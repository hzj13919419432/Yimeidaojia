*{
    padding: 0;
    margin:0 auto;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;

}
body{
  font-size: 12px;
}
p{
  font-size: 12px;

}
h3{
  font-size: 14px;
}
h2{
  font-size: 16px;
}
.classhead{
  margin-top:20px;
  position: absolute;
  top:0px;
  left: 0px;
  padding:10px;
  background: transparent;
}
.classhead.aui-bar-nav.aui-bar-light .aui-iconfont {
    color: #34352f;
}
.classhead.aui-bar.aui-bar-light {
    background-size: 100% 0px;
}
 .headbg img{
  margin-top: 20px;
  width: 100%;
}

main{
    background: #fff;
}
main .title{
  border-bottom: 1px solid #ccc;
}
main  h2{
  background: #fff;
 padding-left: 20px;
 padding-top: 20px;
}
main h3{
  padding-top:20px;
}
main h3,main p{
  padding-left: 20px;
  padding-right: 20px;
  line-height: 30px;

}

main .title .left{
  border-right: 1px solid #000;
  padding-right: 10px;
}
.join{
  margin-top: 60px;
}
.join .aui-btn {
  background: #181818;
  border-radius: 30px;
  color:#83602d;
}

*{
    padding: 0;
    margin:0 auto;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;

}
body{
  font-size: 12px;
}
p{
  font-size: 12px;

}
h3{
  font-size: 14px;
}
.head{
  margin-top: 20px;
  background: #181818;

}
.head .back{
  margin-top:20px;
}
.head>div{

  color:#fff;
  background: #181818;
  padding-top: 30px;
}

.head .aui-tab-item {
  color:#855f2b;
}

.head .aui-tab-item.aui-active {
    color: #a37532;
    font-size: 16px;
    border-bottom: 2px solid #a37532;

}

.activelist ul li img{
  margin:20px auto;
  border-radius: 10px;
}

.activelist li{
  border:1px solid #e5e5e5;

}
.activelist .title{
  padding-left: 20px;
  position: relative;
}
.activelist .title .left{
  border-right: 1px solid #000;
  padding-right: 5px;

}
.activelist .title p{
  margin: 10px 0px;
}

.activelist .join{
  position: absolute;
  bottom: 10px;
  right: 10px;
  width: 100px;
  height: 40px;
  background: #ebebeb;
  border-radius: 20px;
  text-align: center;
  line-height: 40px;
  color:#a37532;


}

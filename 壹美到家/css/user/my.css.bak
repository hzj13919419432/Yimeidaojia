
body{
	background-color: #fff;
	position: relative;
}
section{
	position: absolute;
	left:0;
}
.header{
	width: 100%;
	height:200px;
	background: linear-gradient(#ff722e, #f58a20);
	position: relative;
}
.row-header{
	height:70px;
	text-align: right;
	padding-top: 25px;
	padding-right: 10px;
}
.icon-gear{
	margin-top:10px; 
	position: relative;
	font-family: "aui_iconfont" !important;
	font-size: 20px;
	font-style: normal;
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
	color: #fff
}
.my{
  width: 100%;
  height: 115px;
  display: flex;
  padding-left: 20px;
  padding-right: 20px;
}
.my-avatar{
	height: 60px;
	width: 60px;
	border-radius: 15px;
	margin-left: 15px;
}
.my-title{
	color: #fff;
  font-size: 16px;
  font-weight: 700;
  padding-left: 5px;
  margin-top:10px;
  width:100%;
}
.my-title>div{
	font-size:12px;
	width:100%;
	position: relative;
} 
.my-title>div>span{
	position: absolute; 
	top:2px;left:25px;
	font-weight: 10 !important;
}
.my-title div img{
	width:20px;
}
.header-left{
	display: flex;
	width:80%;
}
.header-right{
	display: flex;
	height:20px;
	background-color: #ecbc44;
	padding:2px 5px;
	border-radius:20px; 
	margin-top:15px;
}
.right img{ 
	width:100%;
}
.aui-img-round.aui-list-img-sm{
	width:20px;
	margin-top: 2px;
}
.aui-list-item-inner{
	font-size: 12px;
	width:100%;
	margin-left:3px; 
	color: #fff;
}
.order{
	padding:0 15px;
}
.order-header{
	color: #000;
	font-weight: 700;
	font-size: 14;
	padding-top: 20px;
}
.btn-order{
	width: 30px;
	height: 30px;
	margin-left: 20px;
}
.order-label{
	font-size: 12px;
}
.golo{
	position: absolute;
	top:160px;left:15px;
	width:90%; 
	background-color: #000;
	line-height:44px; 
	border-radius:10px; 
	padding-bottom:3rem;
}
footer>span#myServerTitle{
	display: inline-block;
	width: 100%;
	height: 30px;
	font-size: 0.9em;
	font-weight: bold;
}

.aui-grid .aui-iconfont{
	font-size: 1.2em;
}
.aui-grid .aui-grid-label{
	font-size: 0.8em;
}
.aui-btn {
    font-weight: 40 !important;
    height:1rem !important; 
    line-height:1rem !important;
	border-radius: 1.2rem !important;
	margin:12px; 
}
.golo div div img{
	width:40px !important; 
	margin:2px 10px;
}
.golo .aui-list-item-title{
	font-size:0.75rem;
	color:#ecbc44; 
}
.golo .aui-list-item-right{
	margin-left:3rem;
	font-size:0.5rem;  
	color:#ecbc44; 
}
footer{
	margin:220px 15px 20px;
}
.aui-col-xs-3 img{
	padding:0 30px;
}
.aui-grid [class*=aui-col-] {
    display: table-cell;
    position: relative;
    text-align: center;
    vertical-align: middle;
    padding: 1rem 0 0 0 !important;
}
footer .aui-col-xs-3 img{
	padding:0 25px;
}
<script type="text/html" id="hmrmzx_tmpl">
	<img src="../../image/public/home/zixun_bg.png" alt="">
	{{each hmrmzx_info as item}}
	<li class="aui-list-item aui-list-item-middle">
		<div class="aui-media-list-item-inner">
			<div class="aui-list-item-media">
				<img src="{{item.P_ImgUrl}}" class="aui-list-img-sm">
			</div>
			<div class="aui-list-item-inner">
				<div class="aui-list-item-text">
					{{item.SL_Title}}
				</div>
				<div class="aui-list-item-text">
					香水/潮流
				</div>
				<div class="aui-list-item-text">
					<div class="aui-list-item-title aui-font-size-12">
						{{item.P_CrtTime}}
					</div>
					<div class="aui-list-item-right">
						<img src="../../image/public/home/yuedu_2x.png" alt="">
					</div>
					<div><a>9999+</a></div>
				</div>
			</div>
		</div>
	</li>
	{{/each}}
</script>
<script type="text/html" id="hmrqtj_tmpl">
{{each hmrqtj_info as item}}
<div class="aui-col-xs-4 texts">
	<img src="{{item.P_ImgUrl}}" />
	<div>
		<p>￥{{item.P_Price}}<a>{{item.P_ShopPrice}}</a></p>
		<div>{{item.P_Name}}</div>
		<div class="imag">
			<img src="../../image/public/home/Brandstore.png" alt="">
			<div>CHANEL</div>
		</div>
	</div>
</div>
{{/each}}
</script>
<script type="text/html" id="hmrmpp_tmpl">
{{each hmrmpp_info as item}}
<div class="aui-slide-node bg-dark">
	<img src="{{item.SL_Image}}" />
	<div class="images">
		<img src="../../image/public/home/Brandstore.png" alt="">
		<div>YSL</div>
	</div>
	<div class="aui-row ysl">
		{{each item.storebrand as storebrand}}
		<div class="aui-col-xs-4">
			<img src="{{storebrand.P_ImgUrl}}" alt="">
			<div>￥{{storebrand.P_Price}} <i>{{storebrand.P_ShopPrice}}</i></div>
			<div>YSL</div>
		</div>
		{{/each}}
	</div>
</div>
{{/each}}
</script>

<script type="text/html" id="topicCat_tmpl">
  {{each topicCat_list as topicCat}}
  <div class="deser">
      {{if topicCat.TC_Image}}
      <img src="{{topicCat.TC_Image}}" />
      {{else}}<img src="../../image/bussschool/bussschool/809752eab1359594.jpg" />
      {{/if}}
      <div class="zhen">
          <span>活动</span>
          <div># {{topicCat.TC_Name}} #</div>
      </div>
  </div>
  {{/each}}
</script>
<script type="text/html" id="topic_tmpl">
{{each topic_list as topic}}
<div class="aui-card-list-user aui-padded-10">
    <div class="aui-card-list-user  aui-padded-10">
        <div class="aui-card-list-user-avatar">
          {{if topic.CU_Photo}}
            <img src="{{topic.CU_Photo}}" class="aui-img-round" />
            {{else}}<img src="../../image/bussschool/bussschool/timg.jpg" class="aui-img-round" />
          {{/if}}
        </div>
        <div class="aui-card-list-user-name">
            <div>{{topic.CU_Nickname ?topic.CU_Nickname:'暂无设置'}}</div>
            <small>
                <i class="aui-iconfont aui-icon-like"></i>
                <i class="aui-iconfont aui-icon-edit"></i>
            </small>
        </div>
        <div class="aui-card-list-user-info">{{topic.TP_CrtTime}}</div>
    </div>
    <div class="aui-card-list code">
        <div class="aui-list-item">
            {{topic.TP_Title}}
        </div>
        <div class="aui-list-item">
            {{topic.TP_Content}}
        </div>
    </div>
    <div class="aui-card-list-user aui-padded-15">
        <div class="seaImg">  <!-- seaImg可动态生成多个 -->
            <div id="picOne">　　　
                <!-- <img src="../../image/bussschool/bussschool/f4eec3a904d8d3c8.jpg" />

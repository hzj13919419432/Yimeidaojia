@charset "utf-8";
*{
    padding: 0;
    margin:0 auto;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
	color: gray;
}
body{
	background-color: #fff;
}
header > *{
	margin-top:20px !important;
}
header .aui-iconfont{
    font-size:16px !important;
    color: #000 !important;
}
header .aui-title{
    color:#000;
}
header{
	position: fixed !important;
    top:0;
}
.aui-content{
    margin-top:65px !important;
}
.aui-card-list-header {
    padding: 0 !important;
}
.aui-card-list-footer > div{
    width: 100%;
    overflow: hidden;
    -webkit-overflow-scrolling: touch;
    overflow-y: hidden;
    overflow-x: scroll;
}
section > div{
    position: relative;

}
section .aui-card-list-footer{
    margin-top:100px;
}
section .aui-card-list-content-padded {
    padding: 1rem 0.75rem;
    box-shadow: 0 0 20px #dddddd;
    -moz-box-shadow: 0 0 20px #dddddd;
    -webkit-box-shadow: 0 0 20px #dddddd;
}
section >div > .aui-card-list-content-padded{
    position: absolute;
    top:180px;
    left:5%;
    width:90%;
    background: #fff;
    z-index:0;
}
section >div > .aui-card-list-content-padded h4{
    font-size:14px;
    font-weight: 700;
    text-align: center;
    margin-bottom: 5px;
    color:#000;
}
section >div > .aui-card-list-content-padded div{
    word-break: break-all;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp:2;
    overflow: hidden;
    font-size: 12px;
    color:#000;
}
.amen{
    width: 900px;
    height: 100%;
}
.amen > div{
    width: 150px;
    padding: 10px 10px 0 !important;
    float: left;
}
.aui-col-xs-4 > *{
    text-align: center;
    margin-top:5px;
}
.aui-col-xs-4 p{
    word-break: break-all;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp:1;
    overflow: hidden;
    font-size:12px;
}
.aui-col-xs-4 div span{
    color:#000;
    font-size:10px;
}
.aui-col-xs-4 div a{
    font-size: 14px;
    font-weight:700;
    color:#000;
}
.aui-col-xs-4 p:nth-child(4){
    font-size:12px;
    text-decoration: line-through;
}
.footer{
    width:100%;
    text-align: center;
    color:#000;
    font-size:14px;
    font-weight: 700;
    margin:1rem 0;
}




.clearfix:after {
    content: ".";
    display: block;
    clear: both;
    visibility: hidden;
    line-height: 0;
    height: 0;
    font-size: 0;
}

.clearfix {
    *zoom: 1;
}

.Cooldog_container {
    width:100%;
    height: 130px;
    position: relative;
    overflow: hidden;
}

.Cooldog_container .Cooldog_content {
    position: absolute;
    width: 100%;
    top: 0;
    left:66%;
    margin-left: -249px;
}

.Cooldog_container .Cooldog_content li {
    position: absolute;
    top: 0;
    left:18%;
    width:70%;
    transition: all 0.3s ease;
}

.Cooldog_container .Cooldog_content li img {
    width: 100%;
    height: 100%;
    vertical-align: middle;
    display: inline-block;
}

.Cooldog_container .btn_left, .Cooldog_container .btn_right {
    display: none;
    width: 126px;
    height: 179px;
    position: absolute;
    top: -30px;
    z-index: 50;
    color: #fff;
    line-height: 179px;

}

.Cooldog_container .btn_left {
    text-align: left;
    left: 0;
}

.Cooldog_container .btn_right {
    text-align: right;
    right: 0;
}

.Cooldog_container i {
    font-size: 50px;
}

.Cooldog_container .btn_close {
    position: absolute;
    top: 0;
    right: 0;
    width: 20px;
    height: 20px;
    display: none;
    line-height: 20px;
    text-align: center;
    background-color: #BCB9B9;
    color: #fff;
}

.Cooldog_container .btn_close i {
    font-size: 18px;
}

.Cooldog_container .buttons {
    width: 370px;
    height: 5px;
    line-height: 5px;
    position: absolute;
    bottom: 5px;
    left: 50%;
    margin-left: -185px;
    z-index: 80;
}

.Cooldog_container .buttons a {
    float: left;
    width: 40px;
    height: 5px;
    background-color: #898F94;
    margin-right: 15px;
}

.Cooldog_container .buttons a.color {
    background-color: #92ffff;
}

.Cooldog_container .buttons a:last-child {
    margin-right: 0;
}


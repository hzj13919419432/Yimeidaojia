@charset "utf-8";
html{
	background-color: #fff;
	width: 100%;
	height: 100%;
}
header{
	position: fixed !important;
	top:0;
	z-index: 589;
}
.aui-bar-nav {
    top: 0;
    line-height: 2.25rem;
    background-color:#000;
    color: #ffffff;
}
header > *{
	margin-top:20px !important;
}
header a span{
	color:#fff;
}
section{
	margin-top:110px;
}
.aui-bar-nav i.aui-iconfont{
	margin-left: 10px;
}
section{
	background-color: #fff;
}
section > div#tab > div.aui-tab-item{
	position: relative;
}
section > div#tab > div.aui-tab-item > span.aui-badge{
	position: absolute;
	right: 0;
	top:-1px;
	background-color: transparent;
	border:1px solid #ececec;
	color:#000;
	text-align: center;
	font-size:12px;
}
div.aui-tab-item.aui-active{
	border-bottom: 2px solid red;
}

section > div.panel{
	display: none;
}
section > div.panel_active{
	display: block;
	background: #f5f5f5;
	border-top: 1px solid #f5f5f5;
}
section > div.panel > div.aui-card-list{
	/*border:1px solid #ddd;*/
}
.aui-card-list {
    position: relative;
    margin-bottom: 0.35rem;
    background: #ffffff;
}
.aui-card-list-header {
    font-size: 0.8rem;
    color: gray;
}
section  div.aui-card-list > div.aui-card-list-header{
	border-bottom: 1px solid #f0f0f0;
	width: 94%;
	margin:0 auto;
	padding-left:0;
	padding-right: 0;
}
section  div.aui-card-list > div.aui-card-list-header > span.aui-iconfont{
	margin-right: 10px;
}
section  div.aui-card-list > div.aui-card-list-header cite{
	color: #ccc;
}
section  div.aui-card-list > div.aui-card-list-content{
	padding:15px;border-bottom: 1px solid #f0f0f0
}
section div.aui-card-list > div.aui-card-list-content > div.aui-col-xs-9 > div:last-child{
	text-align:left;
	color:red;
	margin-top: 8px;
}
section  div.aui-card-list > div.aui-card-list-content img{
	width:70px;
}
section div.aui-card-list > div.aui-card-list-footer > div:first-child{
	width:100%;
}
section div.aui-card-list > div.aui-card-list-footer > div:first-child >div:first-child{
	margin-right:10px;
}
section div.aui-card-list > div.aui-card-list-footer > div:first-child >div:last-child{
	margin:10px 0;
}
section div.aui-card-list > div.aui-card-list-footer input[type=button]{
	float: right;
	border-radius: 15px;
}
.aui-iconfont {
	font-size:16px;
}
.aui-card-list-footer {
	padding: 0.5rem 0.75rem 0;
}
.aui-card-list-header {
    font-size:14px;
}
a{
	color:gray;
}
.aui-card-list-footer{
	margin-bottom:10px !important;
}
.aui-tab{
	position: fixed !important;
	width:100%;
	top:65px;
}
#order_all_div{
	margin-top:110px;
}

section .btncolor{
	color:#fff;
	background: red;
}

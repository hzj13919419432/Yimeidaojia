@charset "UTF-8";
*{
    padding: 0;
    margin:0 auto;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
    font-size: 12px;
}

.head{
  background: #181818;
}
.head>div{
  color:#fff;
  background: #181818;
}

.head .aui-tab-item {
  color:#855f2b;
  font-size: 14px;
}

.head .aui-tab-item.aui-active {
    color: #a37532;
    font-size: 16px;
    border-bottom: 2px solid #a37532;
}
